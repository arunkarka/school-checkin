<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:rotationX="0"
	android:rotationY="0"
	android:rotation="0"
	tools:context=".Student_Marks_ProgressView">
	
	
	<TextView
		android:id="@+id/textView7"
		android:text="@string/curricular_activities"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="130dp"
		android:textSize="25dp"
		android:textAllCaps="true"
		android:textStyle="bold"
		android:layout_marginBottom="10dp"
		app:layout_constraintBottom_toTopOf="@+id/constraintLayout"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />
	
	
	<androidx.constraintlayout.widget.ConstraintLayout
		android:id="@+id/constraintLayout"
		android:layout_width="wrap_content"
		android:textSize="20sp"
		android:layout_height="wrap_content"
		android:layout_marginBottom="30dp"
		app:layout_constraintBottom_toTopOf="@+id/textView5"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.491"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/textView7">
		
		<Button
			android:id="@+id/Button_for_f1"
			android:layout_width="100dp"
			android:layout_height="70dp"
			android:text="@string/test1"
			android:textSize="20sp"
			android:textAllCaps="false"
			android:layout_marginStart="10dp"
			android:layout_marginTop="10dp"
			android:layout_marginEnd="8dp"
			android:layout_marginBottom="2dp"
			app:layout_constraintBottom_toTopOf="@+id/Button_for_f3"
			app:layout_constraintEnd_toStartOf="@+id/Button_for_f2"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" />
		
		
		<Button
			android:id="@+id/Button_for_f2"
			android:layout_width="100dp"
			android:layout_height="70dp"
			android:text="@string/test2"
			android:textSize="20sp"
			android:textAllCaps="false"
			android:layout_marginTop="10dp"
			android:layout_marginStart="8dp"
			android:layout_marginEnd="8dp"
			android:layout_marginBottom="2dp"
			app:layout_constraintBottom_toTopOf="@+id/Button_for_f4"
			app:layout_constraintEnd_toStartOf="@+id/Button_for_s1"
			app:layout_constraintStart_toEndOf="@+id/Button_for_f1"
			app:layout_constraintTop_toTopOf="parent" />
		
		<Button
			android:id="@+id/Button_for_f3"
			android:layout_width="100dp"
			android:layout_height="70dp"
			android:text="@string/test3"
			android:textAllCaps="false"
			android:textSize="20sp"
			android:layout_marginStart="10dp"
			android:layout_marginBottom="10dp"
			android:layout_marginEnd="8dp"
			android:layout_marginTop="2dp"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toStartOf="@+id/Button_for_f4"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/Button_for_f1" />
		
		<Button
			android:id="@+id/Button_for_f4"
			android:layout_width="100dp"
			android:layout_height="70dp"
			android:text="@string/test4"
			android:textAllCaps="false"
			android:textSize="20sp"
			android:layout_marginBottom="10dp"
			android:layout_marginStart="8dp"
			android:layout_marginEnd="8dp"
			android:layout_marginTop="2dp"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toStartOf="@+id/Button_for_s2"
			app:layout_constraintStart_toEndOf="@+id/Button_for_f3"
			app:layout_constraintTop_toBottomOf="@+id/Button_for_f2" />
		
		<Button
			android:id="@+id/Button_for_s2"
			android:layout_width="100dp"
			android:layout_height="70dp"
			android:text="@string/sa_2"
			android:textSize="20sp"
			android:textAllCaps="false"
			android:layout_marginTop="2dp"
			android:layout_marginStart="8dp"
			android:layout_marginEnd="10dp"
			android:layout_marginBottom="10dp"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toEndOf="@+id/Button_for_f4"
			app:layout_constraintTop_toBottomOf="@+id/Button_for_s1" />
		
		<Button
			android:id="@+id/Button_for_s1"
			android:layout_width="100dp"
			android:layout_height="70dp"
			android:text="@string/sa_1"
			android:textSize="20sp"
			android:textAllCaps="false"
			android:layout_marginEnd="10dp"
			android:layout_marginTop="10dp"
			android:layout_marginStart="8dp"
			android:layout_marginBottom="2dp"
			app:layout_constraintBottom_toTopOf="@+id/Button_for_s2"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toEndOf="@+id/Button_for_f2"
			app:layout_constraintTop_toTopOf="parent" />
	
	</androidx.constraintlayout.widget.ConstraintLayout>
	
	<TextView
		android:id="@+id/textView5"
		android:text="@string/co_curricular_activities"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textSize="25dp"
		android:textAllCaps="true"
		android:textStyle="bold"
		android:layout_marginBottom="10dp"
		app:layout_constraintBottom_toTopOf="@+id/constraintLayout1"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/constraintLayout" />
	
	<androidx.constraintlayout.widget.ConstraintLayout
		android:id="@+id/constraintLayout1"
		android:layout_width="wrap_content"
		android:textSize="20sp"
		android:layout_height="wrap_content"
		android:layout_marginBottom="150dp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.497"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/textView5"
		app:layout_constraintVertical_bias="1.0">
		
		
		<Button
			android:id="@+id/Button_for_cc4"
			android:layout_width="100dp"
			android:layout_height="70dp"
			android:text="@string/sa_2"
			android:textSize="20sp"
			android:textAllCaps="false"
			android:layout_marginStart="8dp"
			android:layout_marginTop="10dp"
			android:layout_marginEnd="10dp"
			android:layout_marginBottom="10dp"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toEndOf="@+id/Button_for_cs2"
			app:layout_constraintTop_toTopOf="parent" />
		
		<Button
			android:id="@+id/Button_for_cs2"
			android:layout_width="100dp"
			android:layout_height="70dp"
			android:text="@string/sa_1"
			android:textSize="20sp"
			android:textAllCaps="false"
			android:layout_marginEnd="8dp"
			android:layout_marginTop="10dp"
			android:layout_marginStart="10dp"
			android:layout_marginBottom="10dp"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toStartOf="@+id/Button_for_cc4"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent" />
	
	</androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>